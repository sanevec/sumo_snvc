    <!-- Definición del tipo de vehículo -->

    <vType  id="EV_car" minGap="2.5" maxSpeed="41.66" emissionClass="Energy/unknown" color="green" accel="1" decel="1" sigma="0">
        <param key="has.battery.device" value="true"/>
        <param key="device.battery.capacity" value="64000"/>
        <param key="device.battery.stoppingTreshold" value="0.1"/>
        <param key="allowedPowerIntake" value="120000"/>
        <param key="maximumPower" value="100000"/> <!-- unused -->
        
        <param key="has.stationfinder.device" value="true"/>
        <param key="device.stationfinder.checkEnergyForRoute" value="false"/>
        <param key="device.stationfinder.replacePlannedStop" value="0"/>
        <param key="device.stationfinder.waitForCharge" value="10"/> <!-- reroute if cs is being used, experimental value of 10 works fine -->
        <param key="device.stationfinder.repeat" value="10"/>
        <param key="device.stationfinder.charging.waitingTime.weight" value="3"/>
        
        <param key="airDragCoefficient" value="0.35"/>
        <param key="constantPowerIntake" value="160"/>
        <param key="frontSurfaceArea" value="3"/>
        <param key="rotatingMass" value="0.01"/>      
        <param key="propulsionEfficiency" value="0.9"/>
        <param key="radialDragCoefficient" value="0.5"/>
        <param key="recuperationEfficiency" value="0.9"/>
        <param key="rollDragCoefficient" value="0.01"/>        
        <param key="mass" value="1615"/>        
    </vType>

    <!-- Definición de la ruta -->
    <route id="r0" edges="e1 e2 e3"/>
    <!-- <route id="r1" edges="e1 e4 first_e5 second_e5 e6 e2 e3"/> -->

    <!-- Definición del vehículo -->
    <vehicle id="veh1" type="EV_car" route="r0" depart="0">
        <param key="device.battery.chargeLevel" value="25000"/>        
    </vehicle>
    
    <vehicle id="veh2" type="EV_car" route="r0" depart="20">
        <param key="device.battery.chargeLevel" value="25000"/>
    </vehicle>
    
    <vehicle id="veh3" type="EV_car" route="r0" depart="30">
        <param key="device.battery.chargeLevel" value="25000"/>
    </vehicle>

    <vehicle id="veh4" type="EV_car" route="r0" depart="30">
        <param key="device.battery.chargeLevel" value="25000"/>
    </vehicle>
</routes>

